<script language="javascript">
var token = "medianext";
function redirect()
{
	var query = document.cookie;
	var tmp = query.match(token + "=");
	if(tmp == token.concat("="))
	{
		var next = query.split(token.concat("="));
		var result = next[1].split(";");
		var date = new Date();
		document.cookie = "medianext=expired;expires=" + date ;
		window.frames[0].document.location = result[0];
	}
}
function redirect1()
{
	var query = document.cookie;
	var tmp = query.match(token + "=");
	if(tmp == token.concat("="))
	{
		var next = query.split(token.concat("="));
		var result = next[1].split(";");
		//var date = new Date();
		var date = "Fri, 02-Jan-1970 00:00:00 GMT";
		document.cookie = "medianext=expired;expires=" + date;
		return(result[0].split("\"")[1]);

	} else {
		return("http://media.netvigator.com");
	}
}

</script>

